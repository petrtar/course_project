{
    "i18n-ally.localesPaths": [
        "public/locales",
        "src/shared/config/i18n"
    ],
    "i18n-ally.sourceLanguage": "ru",
    "css.validate": false,
    "less.validate": false,
    "scss.validate": false,
    "stylelint.enable": true,
    "stylelint.validate": ["css", "scss"],
    "editor.codeActionsOnSave": {
        "source.fixAll.stylelint": true
    },
    "i18n-ally.extract.ignoredByFiles": {
        "src/pages/AboutPage/ui/AboutPage.tsx": [
            "О сайте",
            "О сайте"
        ],
        "src/pages/NotFoundPage/ui/NotFoundPage.tsx": [
            "Страница не найдена",
            "Страница не найдена"
        ],
        "src/shared/config/routeConfig/routeConfig.tsx": [
            "*"
        ],
        "src/widgets/PageError/ui/PageError.tsx": [
            "Произошла непредвиденная ошибка"
        ]
    },
    "cSpell.words": [
        "inited"
    ]
}